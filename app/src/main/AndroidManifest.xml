<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mylibraryapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyLibraryApp">
        <activity android:name=".LinkedinActivity"></activity>
        <activity android:name=".WishlistActivity"
            android:label="@string/activity_wishlist"/>
        <activity android:name=".FavoriteBooksActivity"
            android:label="@string/activity_favorites"/>
        <activity android:name=".CurrentlyReadingActivity"
            android:label="@string/activity_currentlyReading"/>
        <activity android:name=".AlreadyReadBooksActivity"
            android:label="@string/activity_alreadyRead"/>
        <activity android:name=".BookActivity"
            android:label="@string/activity_book"/>
        <activity android:name=".AllBooksActivity"
            android:label="@string/activity_allBooks"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>